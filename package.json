{
  "name": "discord-bot-ts",
  "version": "0.0.0",
  "description": "An example of discord bot",
  "main": "src/Bot.ts",
  "scripts": {
    "clean": "ts-node -r tsconfig-paths/register src/CleanBot.ts",
    "start": "ts-node -r tsconfig-paths/register src/Bot.ts",
    "dev": "ts-node -r tsconfig-paths/register src/DevBot.ts",
    "dev:clean": "npm run clean && ts-node -r tsconfig-paths/register src/DevBot.ts",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "author": "FriquetLuca",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^18.14.6",
    "prisma": "^4.11.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5",
    "@prisma/client": "^4.11.0",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3"
  }
}
