{
  "name": "discord-bot-ts",
  "version": "0.0.0",
  "description": "An example of discord bot",
  "main": "src/Bot.ts",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/Bot.ts",
    "start:clean": "ts-node -r tsconfig-paths/register src/Bot.ts clean",
    "dev": "ts-node -r tsconfig-paths/register src/Bot.ts dev",
    "dev:clean": "ts-node -r tsconfig-paths/register src/Bot.ts dev clean",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "author": "FriquetLuca",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/node": "^20.10.4",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "prisma": "^5.7.0"
  }
}
