{
  "name": "discord-bot-ts",
  "version": "0.0.0",
  "description": "An example of discord bot",
  "author": "FriquetLuca",
  "license": "MIT",
  "main": "dist/Bot.js",
  "homepage": "https://github.com/FriquetLuca/discord-bot-ts",
  "scripts": {
    "start": "node -r ./tsconfig-paths.js dist/Bot.js",
    "start:clean": "node -r ./tsconfig-paths.js dist/Bot.js clean",
    "dev": "ts-node -r tsconfig-paths/register src/Bot.ts",
    "dev:clean": "ts-node -r tsconfig-paths/register src/Bot.ts clean",
    "clean": "ts-node -r tsconfig-paths/register src/Bot.ts fullclean",
    "build": "(rm -rf dist && tsc) || tsc",
    "postinstall": "prisma generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "dbt": "ts-node -r tsconfig-paths/register src/db-tests/db.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/node": "^20.10.4",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "prisma": "^5.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
