}

model MHWIMonsterKill {
  id          String  @id @default(cuid())
  user_id     String
  kill_time   BigInt
  monster     MHWIMonsterSpecies
  strenght    MHWIMonsterStrenght
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
